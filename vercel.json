{
    "rewrites": [
      {
        "source": "/api/aiSuggestions",
        "destination": "/api/aiSuggestions.cjs"
      },
      {
        "source": "/api/rewrite",
        "destination": "/api/rewrite.cjs"
      }
    ],
    "functions": {
      "api/**/*.cts": {
        "runtime": "@vercel/node@2.8.0",
        "handler": "api/rewrite.cjs:handler"
      }
    },
    "build": {
      "env": {
        "NODE_ENV": "production"
      }
    }
  }
  